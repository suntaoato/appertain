<?php

//Include conf class so we can use the functions
include "../src/conf.php";

class conf_test extends PHPUnit_Framework_TestCase
{	
	public function testsCanBeRead()
	{
		$conf = new conf("config.ini");
		print($conf->getKey("test","testKey"));
		$this->assertEquals("TestValue", $conf->getKey("test","testKey"));		
	}
}
?>